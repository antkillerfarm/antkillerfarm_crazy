    text centered, minimum height=12em, rounded corners]
\tikzstyle{cunit}=[circle, draw, fill=purple!80, minimum size=1em]
\tikzstyle{cunit0}=[circle, draw, fill=blue!80, minimum size=1em]
\def\blockdist{2.3}
\def\edgedist{2.5}

\begin{tikzpicture}
    \node (naveq) [capsule] {};
    \path (naveq) node (node_0) [cunit] {};
    \path (naveq)+(0,-0.5) node (node_1) [cunit0] {};
\end{tikzpicture}

